type VolatilityForInterval {
  value: Float!
  startTimestamp: Int!
  endTimestamp: Int!
}

type Volatility {
  ticker: String!
  volatilityByInterval: [VolatilityForInterval!]!
  standardDeviation: Float!
}

extend type Query {
  getVolatility(tickers: [String!]!, start: Int!, end: Int!, interval: String!): [Volatility!]!
}

