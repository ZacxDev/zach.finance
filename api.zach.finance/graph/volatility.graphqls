type VolatilityForInterval {
  value: Float!
  startTimestamp: Int!
  endTimestamp: Int!
}

type Volatility {
  volatilityByInterval: [VolatilityForInterval!]!
  standardDeviation: Float!
}

extend type Query {
  getVolatility(ticker: String!, start: Int!, end: Int!, interval: String!): Volatility!
}

